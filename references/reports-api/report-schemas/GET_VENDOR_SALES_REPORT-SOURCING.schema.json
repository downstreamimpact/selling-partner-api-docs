          },
          "shippedCogs": {
            "type": "object",
            "properties": {
              "amount": {
                "type": "number"
              },
              "currencyCode": {
                "type": "string"
              }
            },
            "required": [
              "amount",
              "currencyCode"
            ]
          },
          "shippedRevenue": {
            "type": "object",
            "properties": {
              "amount": {
                "type": "number"
              },
              "currencyCode": {
                "type": "string"
              }
            },
            "required": [
              "amount",
              "currencyCode"
            ]
          },
          "shippedUnits": {
            "type": "integer"
          }
        },
        "required": [
          "customerReturns",
          "endDate",
          "shippedCogs",
          "shippedRevenue",
          "shippedUnits",
          "startDate"
        ]
      }
    },
    "salesByAsin": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startDate": {
            "type": "string"
          },
          "endDate": {
            "type": "string"
          },
          "asin": {
            "type": "string"
          },
          "customerReturns": {
            "type": "integer"
          },
          "shippedCogs": {
            "type": "object",
            "properties": {
              "amount": {
                "type": "number"
              },
              "currencyCode": {
                "type": "string"
              }
            },
            "required": [
              "amount",
              "currencyCode"
            ]
          },
          "shippedRevenue": {
            "type": "object",
            "properties": {
              "amount": {
                "type": "number"
              },
              "currencyCode": {
                "type": "string"
              }
            },
            "required": [
              "amount",
              "currencyCode"
            ]
          },
          "shippedUnits": {
            "type": "integer"
          }
        },
        "required": [
          "asin",
          "customerReturns",
          "endDate",
          "shippedCogs",
          "shippedRevenue",
          "shippedUnits",
          "startDate"
        ]
      }
    }
  },
  "required": [
    "reportSpecification",
    "salesAggregate",
    "salesByAsin"
  ]
